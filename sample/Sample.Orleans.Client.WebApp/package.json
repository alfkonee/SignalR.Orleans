{
  "name": "Sample.Orleans.Client.WebApp",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "start": "dotnet run",
    "start:dev-clean": "npm run clean && npm run build && npm start",
    "start:prod": "dotnet run -p Heroes.Client.WebApp.csproj --launch-profile \"App-Prod\"",
    "start:prod-clean": "npm run clean && npm run build:rel && npm run start-prod",
    "test": "karma start ClientApp/test/karma.conf.js",
    "build:vendor": "node node_modules/webpack/bin/webpack.js --config webpack.config.vendor.js",
    "build:app": "node node_modules/webpack/bin/webpack.js",
    "build": "npm run build:vendor && npm run build:app",
    "build:clean": "npm run clean && npm run build",
    "build:vendor:rel": "npm run build:vendor -- --env.prod",
    "build:app:rel": "npm run build:app -- --env.prod",
    "build:rel": "npm run build:vendor:rel && npm run build:app:rel",
    "clean": "rimraf ./ClientApp/dist wwwroot/dist"
  },
  "dependencies": {
    "@angular/animations": "4.4.6",
    "@angular/common": "4.4.6",
